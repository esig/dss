	<artifactId>dss-jacoco-coverage</artifactId>
	<name>DSS Jacoco coverage</name>
	<description>Module used to collect coverage on all dss modules</description>

	<!--  More details : https://community.sonarsource.com/t/coverage-test-data-importing-jacoco-coverage-report-in-xml-format/12151 -->

	<properties>
		<module-name>jpms_dss_jacoco_coverage</module-name>
	</properties>

	<build>
		<plugins>
			<plugin>
				<groupId>org.jacoco</groupId>
				<artifactId>jacoco-maven-plugin</artifactId>
				<executions>
					<execution>
						<id>report</id>
						<goals>
							<goal>report-aggregate</goal>
						</goals>
						<phase>verify</phase>
					</execution>
				</executions>
				<configuration>
					<excludes>
						<!-- Avoid conflicts between java/java9 classes -->
						<exclude>META-INF/**</exclude>
					</excludes>
				</configuration>
			</plugin>
		</plugins>
	</build>

